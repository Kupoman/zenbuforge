    "actions": [
      {
        "name": "Delete",
        "method": "deleteResource",
        "params": {
          "kind": "projects",
          "key": "/projectList/projects"
        },
        "objectParams": [
          {
            "key": "id",
            "pointer": "/id"
          }
        ]
      },
      {
        "name": "Load",
        "method": "loadProject",
        "objectParams": [
          {
            "key": "id",
            "pointer": "/id"
          }
        ]
      },
      {
        "name": "Import",
        "method": "import",
        "conditions": [
          "isActiveProject"
        ]
      },
      {
        "name": "Export Project",
        "method": "export",
        "conditions": [
          "isActiveProject"
        ]
      },
      {
        "name": "Export glTF",
        "method": "exportGltf",
        "conditions": [
          "isActiveProject"
        ]
      }
    ]
  },
